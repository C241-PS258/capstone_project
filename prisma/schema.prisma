generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// /*
// |--------------------------------------------------------------------------
// | Enum Value
// |--------------------------------------------------------------------------
// */


// /*
// |--------------------------------------------------------------------------
// | Model
// |--------------------------------------------------------------------------
// */

model User {
  id       String     @id @default(uuid())
  name     String     
  email    String?    @unique
  password String
}

model Fish {
    id           Int      @id @default(autoincrement())
    nama         String   @unique
    pakan        String
    pemeliharaan String
}

model Histories {
  id           Int      @id @default(autoincrement())
  image        String
  timestamp    DateTime
}
